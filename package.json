{
  "name": "meo-wifi-login",
  "version": "1.0.3",
  "description": "Allows for an automated login through a MEO Wifi Premium hotspot",
  "main": "lib/meo-wifi-login.js",
  "types": "lib/meo-wifi-login.d.ts",
  "scripts": {
    "babel": "babel src/meo-wifi-login.ts --out-file lib/meo-wifi-login.js",
    "build": "npm run babel; tsc",
    "start": "npm run babel; node lib/meo-wifi-login.js",
    "test-insideNet": "jest -t 'Test Login INSIDE Meo Wifi Network'",
    "test-outsideNet": "jest -t 'Test Login OUTSIDE Meo Wifi Network'",
    "test-withoutNet": "jest -t 'Test Login WITHOUT Network'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darguima/meo-wifi-login-js.git"
  },
  "author": "Darguima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Darguima/meo-wifi-login-js/issues"
  },
  "homepage": "https://github.com/Darguima/meo-wifi-login-js#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@types/crypto-js": "^4.1.1",
    "@types/ip": "^1.1.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "ip": "^1.1.8",
    "jest": "^28.1.0",
    "typescript": "^4.6.4"
  }
}
